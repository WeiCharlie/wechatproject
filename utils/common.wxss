.common_cell{
    position: relative;
    display: flex;
    padding: 15px;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    border-bottom: 1px solid #dadada;
    background-color: white;

   


    
}
.common_cell_hd{
  display: inline-block;
  width: 20px;
  margin-right: 5px;
}
.common_cell_hd image{
  width: 100%;
  height: 20px;
  vertical-align: -2px;
}
.common_cell_bd{
  display: inline-block;
}
.common_cell_bd_p{
  font-size: 14px;
  color: #939393;
}
.badge{
  position: absolute;
  top: 18px;
  right: 40px;
  
  line-height: 15px;
  
  color:#939393;

  text-align: center;
  font-size: 14px;
}

.badge1{
  position: absolute;
  top: 18px;
  left: 50px;
  
  line-height: 15px;
  
  color:#939393;

  text-align: center;
  font-size: 14px;
}

.badge3{
    position: absolute;
  
  right: 10pxpx;
  
  align-items: center;

  line-height: 15px;
  
  color:#939393;

  text-align: center;
  font-size: 14px;
}

.with_arrow{
  position: absolute;
  top: 18px;
  right: 15px;
  width: 15px;
  height: 15px;
  background-image: url(../../images/jian_r.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.common-check {
  position: absolute;
  left: -9999px;
}
.common-check__hd_in-checkbox {
  padding-right: 0.35em;
}
.common-icon-checkbox_circle,
.common-icon-checkbox_success {
  margin-left: 4.6px;
  margin-right: 4.6px;
}

.common-check__label:active {
  background-color: #ECECEC;
}
.common-cells {
  position: relative;
  margin-top: 1.17647059em;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  font-size: 17px;
}